@font-face {
  font-family: ModernSans-Light;
  src: url(ModernSans-Light.otf);

}

body{
	background-color: #2A363B;
	background-image: url("img/gitbacitlogoplainsvg.svg"); 
	background-repeat: no-repeat;
	background-size: 400px 400px;
	background-position: 50% -40%;
	margin: 0;
	padding: 0;
	font-family: ModernSans-Light;
	z-index: 2;
}

h3{
	text-align: center;
}


h2{
	font-size: 35px;
	font-family: ModernSans-Light;
	text-decoration: underline;
}


#start{
	position: absolute;
	height: 30px;
	top: 12%;
	transform: scale(1.4);
	left: 0.3%;
}

#start:hover{
	cursor: pointer;
}

#clockarea{
	position: absolute;
	border-top: 2px outset rgb(59, 59, 59);
	border-left: 2px outset rgb(59, 59, 59);
	border-right: 2px solid rgb(83, 83, 83);
	border-bottom: 2px solid rgb(83, 83, 83);
	width: 10%;
	height: 34px;
	top: 1px;
	left: 45%;
	color: rgb(255, 255, 255);

}

#clock{
	position: absolute;
	font-size: 16px;
	left: 30%;
	top: 25%;
}

#footer{
	position: absolute;
	width: 100%;
	height: 40px;
	top: 0% ;
	background-color: rgb(29, 29, 29);
	border-bottom: 2px outset rgb(71, 71, 71);
}

#documentslogo{
	width: 100%;
	height: 90%;
	position: absolute;
	transform: scale(1.13);
}

#userslogo{
	width: 100%;
	height: 90%;
	position: absolute;
	transform: scale(1.15);
}

#complogo{
	width: 100%;
	height: 90%;
	position: absolute;
}

#trashlogo{
	width: 100%;
	height: 90%;
	position: absolute;
	transform: scale(1.1);
}

#usergrid:hover, #aboutgrid:hover, #contactgrid:hover, #githubgrid:hover{
	cursor: pointer;
}

#start:active{
	opacity: 0.3;
}

#userstag, #abouttag, #contactustag, #githubtag{
	color: white;
	text-align: center;
}

#panel{
	position: absolute;
	width: 40%;
	height: 30%;
	left: 30%;
	top: 20%;
	visibility: collapse;
}


#volumebutton{
	position: absolute;
	width: 20px;
	height: 20px;
	top: 2px;
	left: 4%;
	cursor: pointer;
}


